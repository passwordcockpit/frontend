

<div class="profile-container">
    {{#if this.showChangePasswordMessage}}
        <p class="alert alert-warning">
            <i class="fas fa-exclamation-triangle"></i>
            {{t "Please change your password for the first time you logged in"}}
        </p>
    {{/if}}
    <User::UserDetails @user={{this.model.user}} @isEdit={{true}} @errors={{this.updatingUserErrors}} @isManageUsers={{false}} />
</div>